    &lt;script&gt;
        let scene, camera, renderer, score =3D 0;
        let moveFwd =3D false, moveBwd =3D false, moveL =3D false, moveR =
=3D false;
        let deerArray =3D [];

        function start() {
            document.getElementById('menu').style.display =3D 'none';
            document.body.requestPointerLock();
        }

        // Setup Scene
        scene =3D new THREE.Scene();
        scene.fog =3D new THREE.FogExp2(0x050505, 0.15);
        camera =3D new THREE.PerspectiveCamera(75, window.innerWidth / wind=
ow.innerHeight, 0.1, 1000);
        renderer =3D new THREE.WebGLRenderer({ antialias: false });
        renderer.setSize(window.innerWidth, window.innerHeight);
        document.body.appendChild(renderer.domElement);

        // Floor
        const floor =3D new THREE.Mesh(
            new THREE.PlaneGeometry(200, 200),
            new THREE.MeshLambertMaterial({ color: 0x112211 })
        );
        floor.rotation.x =3D -Math.PI / 2;
        scene.add(floor);

        // Lighting
        const ambient =3D new THREE.AmbientLight(0x404040, 0.5);
        scene.add(ambient);
        const flashlight =3D new THREE.SpotLight(0xffffff, 1.5, 20, 0.4);
        camera.add(flashlight);
        flashlight.position.set(0, 0, 1);
        flashlight.target.position.set(0, 0, -1);
        scene.add(camera);
        scene.add(flashlight.target);

        // Spawn Deer
        function spawnDeer() {
            const geo =3D new THREE.BoxGeometry(0.5, 1, 0.5);
            const mat =3D new THREE.MeshLambertMaterial({ color: 0xffff00 }=
);
            const deer =3D new THREE.Mesh(geo, mat);
            deer.position.set(Math.random() * 40 - 20, 0.5, Math.random() *=
 40 - 20);
            scene.add(deer);
            deerArray.push(deer);
        }
        for(let i=3D0; i&lt;5; i++) spawnDeer();

        // Controls
        document.addEventListener('keydown', (e) =3D&gt; {
            if (e.code =3D=3D=3D 'KeyW') moveFwd =3D true;
            if (e.code =3D=3D=3D 'KeyS') moveBwd =3D true;
            if (e.code =3D=3D=3D 'KeyA') moveL =3D true;
            if (e.code =3D=3D=3D 'KeyD') moveR =3D true;
        });
        document.addEventListener('keyup', (e) =3D&gt; {
            if (e.code =3D=3D=3D 'KeyW') moveFwd =3D false;
            if (e.code =3D=3D=3D 'KeyS') moveBwd =3D false;
            if (e.code =3D=3D=3D 'KeyA') moveL =3D false;
            if (e.code =3D=3D=3D 'KeyD') moveR =3D false;
        });
        document.addEventListener('mousemove', (e) =3D&gt; {
            if (document.pointerLockElement) {
                camera.rotation.y -=3D e.movementX * 0.002;
                camera.rotation.x -=3D e.movementY * 0.002;
            }
        });
        camera.rotation.order =3D 'YXZ';

        function animate() {
            requestAnimationFrame(animate);

            const dir =3D new THREE.Vector3();
            camera.getWorldDirection(dir);
            dir.y =3D 0; dir.normalize();
            const side =3D new THREE.Vector3().crossVectors(camera.up, dir)=
.normalize();

            if (moveFwd) camera.position.addScaledVector(dir, 0.1);
            if (moveBwd) camera.position.addScaledVector(dir, -0.1);
            if (moveL) camera.position.addScaledVector(side, 0.1);
            if (moveR) camera.position.addScaledVector(side, -0.1);

            deerArray.forEach((deer, index) =3D&gt; {
                if (camera.position.distanceTo(deer.position) &lt; 1.5) {
                    scene.remove(deer);
                    deerArray.splice(index, 1);
                    score++;
                    document.getElementById('score').innerText =3D score;
                    spawnDeer();
                   =20
                    if(score &gt; 5) {
                        scene.fog.color.set(0x220000);
                        renderer.setClearColor(0x220000);
                    }
                }
            });

            renderer.render(scene, camera);
        }
        animate();
    &lt;/script&gt;
